{
    "name": "pearl",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/pearl",
    "projectType": "application",
    "targets": {
        "start": {
            "executor": "nx:run-commands",
            "options": {
                "command": "expo start",
                "cwd": "apps/pearl"
            }
        },
        "run-android": {
            "executor": "nx:run-commands",
            "options": {
                "command": "expo start --android",
                "cwd": "apps/pearl"
            }
        },
        "run-ios": {
            "executor": "nx:run-commands",
            "options": {
                "command": "expo start --ios",
                "cwd": "apps/pearl"
            }
        },
        "sync-deps": {
            "executor": "nx:run-commands",
            "options": {
                "command": "expo install",
                "cwd": "apps/pearl"
            }
        },
        "pod-install": {
            "executor": "nx:run-commands",
            "options": {
                "command": "cd ios && pod install",
                "cwd": "apps/pearl"
            }
        },
        "web": {
            "executor": "nx:run-commands",
            "options": {
                "command": "expo start --web",
                "cwd": "apps/pearl"
            }
        },
        "test": {
            "executor": "nx:run-commands",
            "options": {
                "command": "jest --watchAll",
                "cwd": "apps/pearl"
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/pearl/**/*"]
            }
        }
    },
    "tags": []
}
